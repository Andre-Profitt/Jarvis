{
  "name": "jarvis-ai-system",
  "version": "1.0.0",
  "description": "JARVIS AI Integration System with real AI models",
  "main": "src/services/aiModelService.ts",
  "scripts": {
    "start": "ts-node start_jarvis_with_ai.ts",
    "demo": "ts-node src/demo/aiModelDemo.ts",
    "test:ai": "ts-node test_ai_integration.ts",
    "dev": "nodemon --watch src --exec ts-node start_jarvis_with_ai.ts",
    "build": "tsc",
    "setup": "npm install && cp .env.example .env",
    "ollama:start": "ollama serve",
    "python:start": "python3 jarvis_enhanced.py"
  },
  "keywords": [
    "jarvis",
    "ai",
    "assistant",
    "openai",
    "anthropic",
    "ollama",
    "transformers"
  ],
  "author": "JARVIS Team",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "anthropic": "^0.31.2",
    "dotenv": "^16.4.5",
    "ollama": "^0.5.10",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}